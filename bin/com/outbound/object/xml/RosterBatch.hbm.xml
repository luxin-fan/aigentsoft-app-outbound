<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="com.outbound.object">
 <class name="RosterBatch" table="t_roster_batch">
  <id name="id" type="integer" column="id">
     <generator class="identity" />
  </id>
  <property name="batchId" column="batchId" length="50"/>
  <property name="status" column="status" type="integer"/>
  <property name="templateName" column="templateName" length="50"/>
  <property name="activityName" column="activityName" length="50"/>
  <property name="domain" column="domain" length="50"/>
  <property name="createTime" column="createTime" length="50"/>
  <property name="startTime" column="startTime" length="50"/>
  <property name="completeTime" column="completeTime" length="50"/>
  
  <property name="roterNum" column="roterNum" type="integer"/>
  <property name="outCallNum" column="outCallNum" type="integer"/>
  <property name="unCallNum" column="unCallNum" type="integer"/>
  <property name="answerCallNum" column="answerCallNum" type="integer"/>
  <property name="dncNum" column="dncNum" type="integer"/>
   <property name="callRound" column="callRound" type="integer"/>
 </class>
</hibernate-mapping>

