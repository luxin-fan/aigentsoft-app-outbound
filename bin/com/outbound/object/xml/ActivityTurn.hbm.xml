<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="com.outbound.object">
 <class name="ActivityTurn" table="t_activity_turn">
  <id name="id" length="20" column="id">
     <generator class="identity" />
  </id>
  <property name="activityinfoName" column="activityinfoName" length="50" />
  <property name="callResultType" column="callResultType" length="50"/>
  <property name="phoneNumCloumn" column="phoneNumCloumn" length="50" />
   <property name="callTurnInterval" column="callTurnInterval" type="integer"/>
  <property name="callTurnSort" column="callTurnSort" type="integer"/>
  <property name="status" column="status" type="integer"/>
  <property name="creater" column="creater" length="50"/>
  <property name="updater" column="updater" length="50"/>
  <property name="updateTime" column="updateTime" length="50"/>
 </class>
</hibernate-mapping>

